{
    "interface_version": 2,
    "name": "少女前线：云图计划自动化小助手",
    "github": "https://github.com/PinkMMF/MaaGFNeuralCloud",
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb",
            "display_short_side": 720
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "./resource/base"
            ]
        }
    ],
    "task": [
        {
            "name": "启动游戏",
            "entry": "StartGame",
            "default_check": true
        },
        {
            "name": "宿舍收取",
            "entry": "Dormitory",
            "description": "请确保至少添加了5位好友。",
            "default_check": true
        },
        {
            "name": "绿洲收取",
            "entry": "Oasis",
            "default_check": true
        },
        {
            "name": "加工厂领取及生产",
            "entry": "Workshop",
            "description": "该功能只会生产上回订单，请自己先生产一次自己需要的物资。",
            "default_check": true
        },
        {
            "name": "漏洞排查",
            "entry": "ExploreVulnerability",
            "description": "只适配到最高难度，请解锁最高难度快速作战后再使用此功能。",
            "default_check": true
        },
        {
            "name": "故障协议",
            "entry": "ExploreFault",
            "default_check": true
        },
        {
            "name": "碎片搜索",
            "entry": "ExploreFragsSearch",
            "description": "使用该功能前确保碎片搜索中关注且仅关注三个人形。",
            "default_check": true
        },
        {
            "name": "每日双倍",
            "entry": "ExploreDoubleDay",
            "default_check": true
        },
        {
            "name": "商店收取",
            "entry": "Store",
            "option": [
                "故障协议区",
                "漏洞排查区"
            ],
            "default_check": true
        },
        {
            "name": "收集任务奖励",
            "entry": "CollectReward",
            "description": "因每日完成任务会送体力，建议一前一后设置两次该任务",
            "default_check": true
        },
        {
            "name": "资源采集",
            "entry": "ExploreResourceCollect",
            "option": [
                "采集关卡",
                "作战次数"
            ]
        },
        {
            "name": "算法采集",
            "entry": "ExploreAlgorithm",
            "option": [
                "采集及筛取关卡",
                "作战的次数"
            ]
        },
        {
            "name": "收集任务奖励",
            "entry": "CollectReward",
            "description": "因每日完成任务会送体力，建议一前一后设置两次该任务"
        },
        {
            "name": "基础检索",
            "entry": "BasicSearch",
            "default_check": true
        },
        {
            "name": "关闭游戏",
            "entry": "StopGame",
            "default_check": true
        }
    ],
    "option": {
        "采集及筛取关卡": {
            "type": "select",
            "description": "选择想要采集的函数",
            "cases": [
                {
                    "name": "算法采集·进阶",
                    "pipeline_override": {
                        "ExploreAlgorithmAdvanced": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "算法筛取·闪避靶向",
                    "pipeline_override": {
                        "ExploreAlgorithmEnvironment": {
                            "enabled": true
                        },
                        "ExploreAlgorithmEnvironmentDodge": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "算法筛取·暴击靶向",
                    "pipeline_override": {
                        "ExploreAlgorithmEnvironment": {
                            "enabled": true
                        },
                        "ExploreAlgorithmEnvironmentCrit": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "算法筛取·控制靶向",
                    "pipeline_override": {
                        "ExploreAlgorithmEnvironment": {
                            "enabled": true
                        },
                        "ExploreAlgorithmEnvironmentControl": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "算法采集·单格",
                    "pipeline_override": {
                        "ExploreAlgorithmSingular": {
                            "enabled": true
                        }
                    }
                }
            ]
        },
        "作战的次数": {
            "type": "select",
            "description": "作战几次",
            "cases": [
                {
                    "name": "作战1次",
                    "pipeline_override": {
                        "ExploreAlgorithmBack": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "作战2次",
                    "pipeline_override": {
                        "ExploreAlgorithmBattleHit": {
                            "enabled": true,
                            "max_hit": 1
                        },
                        "ExploreAlgorithmBack": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "作战3次",
                    "pipeline_override": {
                        "ExploreAlgorithmBattleHit": {
                            "enabled": true,
                            "max_hit": 2
                        },
                        "ExploreAlgorithmBack": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "作战4次",
                    "pipeline_override": {
                        "ExploreAlgorithmBattleHit": {
                            "enabled": true,
                            "max_hit": 3
                        },
                        "ExploreAlgorithmBack": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "体力耗尽",
                    "pipeline_override": {
                        "ExploreAlgorithmBack": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "采集关卡": {
            "type": "select",
            "description": "选择想要采集的资源",
            "cases": [
                {
                    "name": "底格币",
                    "pipeline_override": {
                        "ExploreResourceCollectDigg": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "作战经验",
                    "pipeline_override": {
                        "ExploreResourceCollectEXP": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "技能阵域",
                    "pipeline_override": {
                        "ExploreResourceCollectSkill": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "潜能特训医师",
                    "pipeline_override": {
                        "ExploreResourceCollectTalent": {
                            "enabled": true
                        },
                        "ExploreResourceCollectTalentGoDoctor": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "潜能特训特种",
                    "pipeline_override": {
                        "ExploreResourceCollectTalent": {
                            "enabled": true
                        },
                        "ExploreResourceCollectTalentGoSpecialist": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "潜能特训射手",
                    "pipeline_override": {
                        "ExploreResourceCollectTalent": {
                            "enabled": true
                        },
                        "ExploreResourceCollectTalentGoMarksman": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "潜能特训战士",
                    "pipeline_override": {
                        "ExploreResourceCollectTalent": {
                            "enabled": true
                        },
                        "ExploreResourceCollectTalentGoWarrior": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "潜能特训守卫",
                    "pipeline_override": {
                        "ExploreResourceCollectTalent": {
                            "enabled": true
                        },
                        "ExploreResourceCollectTalentGoGuardian": {
                            "enabled": true
                        }
                    }
                }
            ]
        },
        "作战次数": {
            "type": "select",
            "description": "选择作战次数",
            "cases": [
                {
                    "name": "作战1次",
                    "pipeline_override": {
                        "ExploreResourceCollectBack": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "作战2次",
                    "pipeline_override": {
                        "ExploreResourceCollectBattleHit": {
                            "enabled": true,
                            "max_hit": 1
                        },
                        "ExploreResourceCollectBack": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "作战3次",
                    "pipeline_override": {
                        "ExploreResourceCollectBattleHit": {
                            "enabled": true,
                            "max_hit": 2
                        },
                        "ExploreResourceCollectBack": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "作战4次",
                    "pipeline_override": {
                        "ExploreResourceCollectBattleHit": {
                            "enabled": true,
                            "max_hit": 3
                        },
                        "ExploreResourceCollectBack": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "体力耗尽",
                    "pipeline_override": {
                        "ExploreResourceCollectBack": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "故障协议区": {
            "type": "select",
            "description": "选择你想兑换的物品",
            "cases": [
                {
                    "name": "心智碎片",
                    "pipeline_override": {
                        "StoreFaultShopFrags": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "极限突破控件·医师",
                    "pipeline_override": {
                        "StoreFaultShopEXdoctor": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "极限突破控件·特种",
                    "pipeline_override": {
                        "StoreFaultShopEXspecialist": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "极限突破控件·射手",
                    "pipeline_override": {
                        "StoreFaultShopEXmarksman": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "极限突破控件·守卫",
                    "pipeline_override": {
                        "StoreFaultShopEXguardian": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "极限突破控件·战士",
                    "pipeline_override": {
                        "StoreFaultShopEXwarrior": {
                            "enabled": true
                        }
                    }
                }
            ]
        },
        "漏洞排查区": {
            "type": "select",
            "description": "选择你想要兑换的物品",
            "cases": [
                {
                    "name": "心智碎片",
                    "pipeline_override": {
                        "StoreVulnerabilityShopFrags": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "医师",
                    "pipeline_override": {
                        "StoreVulnerabilityShopEXdoctor": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "特种",
                    "pipeline_override": {
                        "StoreVulnerabilityShopEXspecialist": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "射手",
                    "pipeline_override": {
                        "StoreVulnerabilityShopEXmarksman": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "守卫",
                    "pipeline_override": {
                        "StoreVulnerabilityShopEXguardian": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "战士",
                    "pipeline_override": {
                        "StoreVulnerabilityShopEXwarrior": {
                            "enabled": true
                        }
                    }
                }
            ]
        }
    }
}